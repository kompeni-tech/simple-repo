{
    "name": "madulinux/repository",
    "description": "Simple make:repository laravel design pattern",
    "type": "library",
    "require": {
        "illuminate/config": "~5.0|~6.0|~7.0|^8.0",
        "illuminate/support": "~5.0|~6.0|~7.0|^8.0",
        "illuminate/database": "~5.0|~6.0|~7.0|^8.0",
        "illuminate/filesystem": "~5.0|~6.0|~7.0|^8.0",
        "illuminate/console": "~5.0|~6.0|~7.0|^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Syaiful Rahman",
            "email": "madulinux@gmail.com"
        }
    ],
    "support": {
        "email": "madulinux@gmail.com"
    },
    "autoload": {
        "psr-4": {
            "Madulinux\\": "src/Madulinux/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Madulinux\\Repositories\\Providers\\RepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
