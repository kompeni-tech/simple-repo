{
    "name": "madulinux/repository",
    "description": "Laravel repository pattern",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/config": ">=6.20.26",
        "illuminate/support": ">=6.20.26",
        "illuminate/database": ">=6.20.26",
        "illuminate/filesystem": >="6.20.26",
        "illuminate/console": ">=6.20.26"
    },
    "autoload": {
        "psr-4": {
            "Madulinux\\": "src/Madulinux/"
        }
    },
    "authors": [
        {
            "name": "Madulinux",
            "email": "madulinux@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Madulinux\\Repositories\\Providers\\RepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
